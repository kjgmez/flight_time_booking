<p id="yoyo"></p>
<%= form_for :appointment do |f|  %>
  <%= f.collection_select :location_id, Location.order(:name), :id, :name, include_blank: true %>
  <%= f.grouped_collection_select :coach_id, Location.order(:name), :coaches, :name, :id, :name_and_level, include_blank: true %>
  <%= f.datetime_select :arrival_time, prompt: {day: "Choose Day", month: "Choose Month", year: "Choose Year", hour: true, minute: true}, ampm: true %>
<% end %>